			Label: "[i:876]海盗套装重做"
			Tooltip: 启用后，由海盗出售的[i:876]海盗时装 具有实际盔甲效果
		}

		gloveDamageType: {
			Label: 手套/标枪职业伤害
			Tooltip:
				'''
				该伤害类型将应用于手套/标枪武器
				如果未启用《灾厄》模组，且设置为盗贼，则默认为远程伤害
				请在Steam创意工坊上评论任何你认为应该添加的伤害类型
				'''
		}

		critScaling: {
			Label: 暴击倍率计算方式
			Tooltip:
				'''
				根据暴击伤害/几率加成，决定暴击伤害的计算方式
				最推荐“平衡”模式，但如果你更喜欢“简单”模式的话，它更强大且易于理解
				平衡：（（武器伤害倍率-1）*（（100+武器暴击率）/99）/2+1）+1)*(1+(（100+武器暴击率）/99)/2)
				简单：武器伤害倍率 * （1+武器暴击率）
				'''
		}

		bossLife: {
			Label: Boss血量倍率
			Tooltip:
				'''
				模组启用且你想的话，允许你增加Boss的血量
				以防你觉得此模组过于强力，想要削弱它
				'''
		}

		enemyLife: {
			Label: 敌怪血量倍率
			Tooltip:
				'''
				模组启用且你想的话，允许你增加非Boss敌怪的血量
				以防你觉得此模组过于强力，想要削弱它
				'''
		}
	}
}

